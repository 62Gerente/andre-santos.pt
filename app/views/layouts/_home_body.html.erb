<body class="background-dark-blue sidebar-hidden">
<%= render "home/sidebar"  %>
<div class="wrap sidebar-icons">
  <%=render "home/header"  %>
  <div class="content container-fluid">
    <%= yield %>
  </div>
</div>
<script>
    $.fn.tagcloud.defaults = {
        size: {start: 11, end: 37, unit: 'px'},
        color: {start: '#48687E', end: '#fff'}
    };

    $(function () {
        $('#cenas a').tagcloud();
    });
</script>
<script>
    $(document).ready(function() {
        $('#load_more_tweets_link').click(function(event) {
            event.preventDefault();
            $(this).hide();
            $('#load_more_tweets_div').show();
        });
    });
</script>
</body>